{
  "name": "hyperf-plus/gen-chat-plugin",
  "description": "A OpenAI plugin yaml.",
  "license": "MIT",
  "keywords": [
    "php",
    "swoole",
    "hyperf",
    "swagger"
  ],
  "support": {
  },
  "require": {
    "php": ">=8.0",
    "hyperf-plus/route": "^3.0.0"
  },
  "require-dev": {
  },
  "suggest": {
  },
